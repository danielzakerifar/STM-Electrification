package FirstPackage;
import java.sql.Time;
import java.util.ArrayList;

public class BusCharged {
	public ArrayList<Bus>  busCharged;     
	public ArrayList<Time> startSchedule;  
	public ArrayList<Time> finishedSchedule;
	public ArrayList<Route>Next_Trip;

	public BusCharged() {
		this.busCharged=new ArrayList<Bus>(10);
		this.startSchedule=new ArrayList<Time>(10);
		this.finishedSchedule=new ArrayList<Time>(10);
		this.Next_Trip=new ArrayList<Route>(10);
		
	}
	public void showSchedule() {
		for(int i=0;i<this.startSchedule.size();i++) {
			System.out.println(this.busCharged.get(i).id+" \t"+this.startSchedule.get(i)+"\t "+this.finishedSchedule.get(i)+"\t"+this.Next_Trip.get(i).Id);
			
			
		}
		
		
		
	}
	
	

}
